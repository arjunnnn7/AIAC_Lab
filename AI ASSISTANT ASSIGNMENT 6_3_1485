        "#Task 4: Sum of First n Natural Numbers\n",
        "def sum_to_n_for(n):\n",
        "    total = 0\n",
        "    for i in range(1, n + 1):\n",
        "        total += i\n",
        "    return total\n",
        "\n",
        "\n",
        "print(sum_to_n_for(10))\n",
        "def sum_to_n_while(n):\n",
        "    total = 0\n",
        "    i = 1\n",
        "    while i <= n:\n",
        "        total += i\n",
        "        i += 1\n",
        "    return total\n",
        "\n",
        "\n",
        "print(sum_to_n_while(10))\n",
        "def sum_to_n_formula(n):\n",
        "    return n * (n + 1) // 2\n",
        "\n",
        "\n",
        "print(sum_to_n_formula(10))"
      ],
