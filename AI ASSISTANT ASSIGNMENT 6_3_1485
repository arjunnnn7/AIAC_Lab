        "#Task 2: Loops – First 10 Multiples of a Number\n",
        "def print_multiples_for(num):\n",
        "    print(f\"First 10 multiples of {num}:\")\n",
        "    for i in range(1, 11):\n",
        "        print(num * i)\n",
        "\n",
        "\n",
        "print_multiples_for(5)\n",
        "def print_multiples_while(num):\n",
        "    i = 1\n",
        "    print(f\"First 10 multiples of {num}:\")\n",
        "    while i <= 10:\n",
        "        print(num * i)\n",
        "        i += 1\n",
        "\n",
        "\n",
        "print_multiples_while(5)"
      ],
