        "#Task 3: Conditional Statements – Age Classification\n",
        "def classify_age(age):\n",
        "    if age < 13:\n",
        "        return \"Child\"\n",
        "    elif age < 20:\n",
        "        return \"Teenager\"\n",
        "    elif age < 60:\n",
        "        return \"Adult\"\n",
        "    else:\n",
        "        return \"Senior\"\n",
        "\n",
        "\n",
        "print(classify_age(10))\n",
        "print(classify_age(17))\n",
        "print(classify_age(25))\n",
        "print(classify_age(65))"
