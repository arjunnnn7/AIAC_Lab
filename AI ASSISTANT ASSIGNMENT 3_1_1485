      "cell_type": "code",
      "source": [
        "#Question 3: Few-Shot Prompting\n",
        "def is_armstrong(num):\n",
        "    digits = len(str(num))\n",
        "    total = 0\n",
        "    temp = num\n",
        "\n",
        "    while temp > 0:\n",
        "        digit = temp % 10\n",
        "        total += digit ** digits\n",
        "        temp //= 10\n",
        "\n",
        "    return total == num\n",
        "\n",
        "n = int(input(\"Enter a number: \"))\n",
        "if is_armstrong(n):\n",
        "    print(\"Armstrong Number\")\n",
        "else:\n",
        "    print(\"Not an Armstrong Number\")"
      ],
      "metadata": {
