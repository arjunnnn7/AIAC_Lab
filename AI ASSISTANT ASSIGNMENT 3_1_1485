      "cell_type": "code",
      "source": [
        "#Question 5: Zero-Shot Prompting\n",
        "def is_perfect(num):\n",
        "    if num <= 0:\n",
        "        return False\n",
        "\n",
        "    total = 0\n",
        "    for i in range(1, num):\n",
        "        if num % i == 0:\n",
        "            total += i\n",
        "\n",
        "    return total == num\n",
        "\n",
        "\n",
        "n = int(input(\"Enter a number: \"))\n",
        "if is_perfect(n):\n",
        "    print(\"Perfect Number\")\n",
        "else:\n",
        "    print(\"Not a Perfect Number\")"
      ],
